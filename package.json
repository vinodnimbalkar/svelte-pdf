{
  "name": "@defenseunicorns/svelte-pdf",
  "version": "1.0.1",
  "description": "svelte-pdf provides a component for rendering PDF documents using PDF.js",
  "keywords": [
    "pdf",
    "svelte-pdf",
    "pdfjs",
    "pdf-print",
    "pdf-zoom",
    "pdf-rotation",
    "pdf-navigation"
  ],
  "homepage": "https://github.com/defenseunicorns/svelte-pdf#readme",
  "bugs": {
    "url": "https://github.com/defenseunicorns/svelte-pdf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defenseunicorns/svelte-pdf.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "main": "index.js",
  "svelte": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && svelte-kit sync && svelte-package",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "clean": "rimraf dist",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "package": "svelte-kit package",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "echo 'Add tests'"
  },
  "dependencies": {
    "@fontsource-variable/montserrat": "5.0.17",
    "@fontsource/montserrat": "5.0.16",
    "pdfjs-dist": "4.0.379"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@sveltejs/adapter-auto": "3.1.1",
    "@sveltejs/kit": "2.5.2",
    "@sveltejs/package": "2.2.7",
    "autoprefixer": "10.4.17",
    "cssnano": "6.0.5",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.2",
    "rimraf": "5.0.5",
    "svelte": "4.2.12",
    "svelte-check": "3.6.5",
    "svelte-preprocess": "5.1.3",
    "svelte2tsx": "0.7.3",
    "typescript": "5.3.3",
    "vite": "5.1.4"
  },
  "peerDependencies": {
    "svelte": "4.x"
  }
}
